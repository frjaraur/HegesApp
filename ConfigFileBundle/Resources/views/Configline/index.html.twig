<p><h1 class="tabla_datos">Linea de Configuracion - LISTAR</h1></p>
<form action="{{ path('configline_new') }}"><button type="submit">CREATE A NEW ENTRY</button></form>
<table><tr><th class="tabla_datos">Id</th>
        <td class="tabla_datos_sin_borde" colspan="2"> </th></tr>
{% for entity in entities %}
    <tr><td class="tabla_datos"><a href="{{ path('configline_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
        <td class="tabla_datos_sin_borde"><form action="{{ path('configline_show', { 'id': entity.id }) }}"><button type="submit">MOSTRAR</button></form></td>
        <td class="tabla_datos_sin_borde"><form action="{{ path('configline_edit', { 'id': entity.id }) }}"><button type="submit">EDITAR</button></form></td>
    </tr>
{% endfor %}
</table>